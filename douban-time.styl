.anony-time
  font-size 24px
  padding: 40px 0 20px
  margin 0 0 30px
  background-color #f7f7f7
  .wrapper
    width 950px
    margin 0 auto
    .sidenav
      margin-right 20px
      display inline-block
      vertical-align top
      .timeTitle
        margin-bottom 12px
        a
          font-size 24px
          color #494949

    .main
      display inline-block
      width 850px
      .section-title
        color #072
        font-size 15px
      .timelist
        li
          display inline-block
          width 100px
          margin-right 40px
          margin-bottom 30px
          text-align center
          vertical-align top
          .cover,.time-audio,.new
            display block
            margin-bottom 10px
            // position relative
          .title
            font-size 13px
            color #333
            line-height 1.38
          .musiclm
            display block
            color #999999
            font-size 12px
            margin-top 10px




      



